<div class="error">
  <%= error_messages_for :vlan %>
</div>

<%= form_for([:admin, @vlan]) do |f| %>
<table class="entry_form" align="center">
  <caption>Edit VLAN</caption>
  <tfoot>
    <tr>
      <td></td>
      <td colspan="1">
        <input type='submit' value="Save changes" />
      </td>
    </tr>
  </tfoot>
  <%= render :partial => 'form', :locals => { :f => f } %>
</table>
<% end %>

&laquo; <%= link_to('Back to All VLANS', admin_vlans_path) %>
